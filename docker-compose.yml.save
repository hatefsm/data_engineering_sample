version: "3.9"

services:
  source_db:
    image: postgres:16
    container_name: etraveli_source_db
    environment:
      POSTGRES_USER: demo_user
      POSTGRES_PASSWORD: demo_pass
      POSTGRES_DB: etraveli_source
    ports:
      - "5432:5432"
    volumes:
      - ./sql:/docker-entrypoint-initdb.d
